<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/queueingPage"
    tools:context="com.example.zackwang.testapp.QueueingActivity">

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:id="@+id/tableRow">

        <EditText
            android:layout_width="96dp"
            android:layout_height="8dp"
            android:id="@+id/editText"
            android:background="#7ff5b9"
            android:layout_gravity="top|center_vertical"
            android:layout_column="0" />

        <EditText
            android:layout_width="96dp"
            android:layout_height="8dp"
            android:id="@+id/editText2"
            android:background="#4ac1bd"
            android:layout_gravity="top|center_vertical"
            android:layout_column="1" />

        <EditText
            android:layout_width="96dp"
            android:layout_height="8dp"
            android:id="@+id/editText3"
            android:background="#f63c54"
            android:layout_gravity="top|center_vertical"
            android:layout_column="2" />

        <EditText
            android:layout_width="96dp"
            android:layout_height="8dp"
            android:id="@+id/editText4"
            android:background="#feb848"
            android:layout_gravity="top|center_vertical"
            android:layout_column="3" />

    </TableRow>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bookIcon"
        android:layout_marginTop="78dp"
        android:src="@mipmap/lg_book"
        android:layout_marginLeft="90dp"
        android:layout_marginStart="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bookr"
        android:id="@+id/logoText"
        android:layout_toRightOf="@+id/bookIcon"
        android:fontFamily="courier"
        android:textSize="50dp"
        android:layout_alignTop="@+id/bookIcon"
        android:layout_alignBottom="@+id/bookIcon" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="________________________________________________________"
        android:id="@+id/loginLine"
        android:focusableInTouchMode="false"
        android:fontFamily="aril"
        android:textColor="#756f7c"
        android:layout_below="@+id/bookIcon"
        android:layout_alignRight="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:layout_marginTop="171dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:id="@+id/register_username"
        android:fontFamily="Arial"
        android:textColor="#756f7c"
        android:layout_below="@+id/loginLine"
        android:layout_marginTop="20dp"
        android:layout_toLeftOf="@+id/imageView4"
        android:layout_toRightOf="@+id/textView9"
        android:layout_toEndOf="@+id/textView9" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:src="@mipmap/nv_menu"
        android:layout_below="@+id/tableRow"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:clickable="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:onClick="queueing_DirectToHomeMenu"
        android:layout_below="@+id/tableRow"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@mipmap/nv_back"
        android:clickable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="________________________________________________________"
        android:id="@+id/tp_line"
        android:focusableInTouchMode="false"
        android:fontFamily="aril"
        android:textColor="#756f7c"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Queueing"
        android:id="@+id/qt_title1"
        android:textColor="#756f7c"
        android:singleLine="false"
        android:textSize="20dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/loginLine"
        android:layout_alignStart="@+id/loginLine" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:id="@+id/textView4"
        android:textColor="#756f7c"
        android:singleLine="false"
        android:textSize="20dp"
        android:layout_alignTop="@+id/register_username"
        android:layout_alignLeft="@+id/tp_line"
        android:layout_alignStart="@+id/tp_line" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="People No."
        android:id="@+id/textView6"
        android:textColor="#756f7c"
        android:singleLine="false"
        android:textSize="20dp"
        android:layout_alignTop="@+id/register_title"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:id="@+id/register_title"
        android:fontFamily="Arial"
        android:textColor="#756f7c"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/register_username"
        android:layout_alignRight="@+id/register_username"
        android:layout_alignEnd="@+id/register_username"
        android:layout_alignLeft="@+id/register_username"
        android:layout_alignStart="@+id/register_username" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone"
        android:id="@+id/textView7"
        android:textColor="#756f7c"
        android:singleLine="false"
        android:textSize="20dp"
        android:layout_alignTop="@+id/register_fullname"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:id="@+id/register_fullname"
        android:fontFamily="Arial"
        android:textColor="#756f7c"
        android:layout_below="@+id/register_title"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/register_title"
        android:layout_alignStart="@+id/register_title"
        android:layout_alignRight="@+id/register_title"
        android:layout_alignEnd="@+id/register_title" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share Table"
        android:id="@+id/textView9"
        android:textColor="#756f7c"
        android:singleLine="false"
        android:textSize="20dp"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/register_fullname"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <Button
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:text="Cancel"
        android:id="@+id/tp_cancelBtn"
        android:onClick="queueing_DirectToHomeMenu"
        android:background="#ee5f45"
        android:fontFamily="Arial"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_below="@+id/tp_line3"
        android:layout_alignLeft="@+id/tp_line3"
        android:layout_alignStart="@+id/tp_line3"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="________________________________________________________"
        android:id="@+id/tp_line3"
        android:focusableInTouchMode="false"
        android:fontFamily="aril"
        android:textColor="#756f7c"
        android:layout_below="@+id/textView9"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

    <Button
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:text="Apply"
        android:id="@+id/button3"
        android:onClick="queueing_DirectToQueueTime"
        android:background="#34cf95"
        android:fontFamily="Arial"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/tp_cancelBtn"
        android:layout_alignRight="@+id/tp_line3"
        android:layout_alignEnd="@+id/tp_line3" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageView5"
        android:src="@mipmap/ic_right"
        android:layout_alignTop="@+id/register_username"
        android:layout_alignRight="@+id/loginLine"
        android:layout_alignEnd="@+id/loginLine" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageView10"
        android:src="@mipmap/ic_minus"
        android:layout_alignTop="@+id/register_title"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView11"
        android:layout_alignTop="@+id/textView9"
        android:layout_alignLeft="@+id/register_fullname"
        android:layout_alignStart="@+id/register_fullname" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView12"
        android:src="@mipmap/ic_switch1"
        android:layout_toRightOf="@+id/imageView11"
        android:layout_toEndOf="@+id/imageView11"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="0dp"
        android:layout_below="@+id/register_fullname" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView13"
        android:src="@mipmap/ic_switch0"
        android:clickable="true"
        android:alpha="1"
        android:adjustViewBounds="false"
        android:layout_below="@+id/register_fullname"
        android:layout_alignLeft="@+id/register_fullname"
        android:layout_alignStart="@+id/register_fullname" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageView14"
        android:src="@mipmap/ic_right"
        android:layout_alignTop="@+id/register_fullname"
        android:layout_alignLeft="@+id/imageView10"
        android:layout_alignStart="@+id/imageView10" />

</RelativeLayout>
